{
  "name": "vue-sfp-services",
  "version": "2.0.0",
  "description": "Package allows us to use pagination, search and filter easily made with VueJS",
  "author": {
    "name": "Sohaib Alslamat",
    "email": "sohep.alslamat91@gmail.com",
    "url": "https://github.com/sohepalslamat",
    "company": "Vuango"
  },
  "keywords": [
    "filter",
    "pagination",
    "search",
    "vue2",
    "vue3",
    "vuejs",
    "Vuango"
  ],
  "files": [
    "dist/*"
  ],
  "main": "./dist/vue-sfp-services.umd.js",
  "module": "./dist/vue-sfp-services.es.js",
  "browser": "./dist/vue-sfp-services.es.js",
  "types": "./dist/vue-sfp-services.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-sfp-services.es.js",
      "require": "./dist/vue-sfp-services.umd.js"
    }
  },
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "deploy": "sh ./deploy.sh"
  },
  "dependencies": {
    "vue": "^2.7.7"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "gh-pages": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "terser": "^5.14.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.2",
    "vite-plugin-dts": "^1.7.1",
    "vue-router": "^3.5.4",
    "vue-tsc": "^0.38.8"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sohepalslamat/vue-sfp-services"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
