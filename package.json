{
  "name": "vue-sfp-services",
  "version": "2.0.1",
  "description": "Package allows us to use pagination, search and filter easily made with VueJS",
  "author": {
    "name": "Sohaib Alslamat",
    "email": "sohep.alslamat91@gmail.com",
    "url": "https://github.com/sohepalslamat",
    "company": "Vuango"
  },
  "keywords": [
    "filter",
    "pagination",
    "search",
    "vue2",
    "vue3",
    "vuejs",
    "Vuango"
  ],
  "files": [
    "dist/*"
  ],
  "main": "./dist/vue-sfp-services.umd.js",
  "module": "./dist/vue-sfp-services.es.js",
  "browser": "./dist/vue-sfp-services.es.js",
  "types": "./dist/vue-sfp-services.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-sfp-services.es.js",
      "require": "./dist/vue-sfp-services.umd.js"
    }
  },
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "deploy": "sh ./deploy.sh"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8",
    "gh-pages": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue-router": "^4.2.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sohepalslamat/vue-sfp-services"
  }
}
